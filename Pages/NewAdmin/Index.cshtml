@page
@model FundingDashboardAPI.Pages.NewAdmin.IndexModel


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Funding[0].Name)
            </th>
            @*<th>
                @Html.DisplayNameFor(model => model.Funding[0].Region)
            </th>*@
            <th>
                @Html.DisplayNameFor(model => model.Funding[0].ServiceLine)
            </th>
            @*<th>
                @Html.DisplayNameFor(model => model.Funding[0].MinimumCriteria)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Funding[0].Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Funding[0].HowToApply)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Funding[0].WhenToApply)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Funding[0].ApprovalProcess)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Funding[0].FundingCollection)
            </th>*@
            <th>
                @Html.DisplayNameFor(model => model.Funding[0].AddedBy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Funding[0].AddedOn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Funding[0].Archived)
            </th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Funding) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            @*<td>
                @Html.DisplayFor(modelItem => item.Region)
            </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.ServiceLine)
            </td>
            @*<td>
                @Html.DisplayFor(modelItem => item.MinimumCriteria)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HowToApply)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WhenToApply)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ApprovalProcess)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FundingCollection)
            </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.AddedBy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AddedOn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Archived)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
